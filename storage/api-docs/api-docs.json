{
    "openapi": "3.0.0",
    "info": {
        "title": "Coredump API",
        "description": "",
        "contact": {
            "email": "info@coredump.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/data/users": {
            "get": {
                "summary": "Mostrar usuarios",
                "responses": {
                    "200": {
                        "description": "Mostrar todos los usuarios."
                    },
                    "default": {
                        "description": "Lista de usuarios."
                    }
                }
            }
        },
        "/api/data/user": {
            "post": {
                "tags": [
                    "jdd-menus"
                ],
                "summary": "Obtiene los menues del usuario",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "call": {
                                        "properties": {
                                            "method": {
                                                "type": "string",
                                                "example": "allMenus"
                                            },
                                            "parameters": {
                                                "type": "object"
                                            }
                                        },
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Process"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/data/process": {
            "get": {
                "tags": [
                    "jdd-workflow"
                ],
                "summary": "Muestra una lista de instancias de procesos",
                "responses": {
                    "200": {
                        "description": "Lista de instancias de procesos.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Process"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "jdd-workflow"
                ],
                "summary": "Guarda una nueva instancia de proceso",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "data": {
                                        "type": "object",
                                        "$ref": "#/components/schemas/ProcessEditable"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Process"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/data/test": {
            "get": {
                "tags": [
                    "jdd-test"
                ],
                "summary": "List some test records",
                "responses": {
                    "200": {
                        "description": "List some test records.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Test"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "jdd-test"
                ],
                "summary": "Save a test record",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "data": {
                                        "type": "object",
                                        "$ref": "#/components/schemas/TestEditable"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Test"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ProcessEditable": {
                "properties": {
                    "attributes": {
                        "description": "Process model\n\nSwagger definition:",
                        "properties": {
                            "bpmn": {
                                "description": "Process model\n\nSwagger definition:",
                                "type": "string"
                            },
                            "data": {
                                "description": "Process model\n\nSwagger definition:",
                                "type": "object"
                            },
                            "tokens": {
                                "description": "Process model\n\nSwagger definition:",
                                "type": "array",
                                "items": {
                                    "properties": {
                                        "element": {
                                            "description": "Process model\n\nSwagger definition:",
                                            "type": "string"
                                        },
                                        "name": {
                                            "description": "Process model\n\nSwagger definition:",
                                            "type": "string"
                                        },
                                        "implementation": {
                                            "description": "Process model\n\nSwagger definition:",
                                            "type": "string"
                                        },
                                        "status": {
                                            "description": "Process model\n\nSwagger definition:",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            },
                            "status": {
                                "description": "Process model\n\nSwagger definition:",
                                "type": "string",
                                "enum": [
                                    "ACTIVE",
                                    "COMPLETED"
                                ]
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "Process": {
                "allOf": [
                    {
                        "properties": {
                            "id": {
                                "description": "Process model\n\nSwagger definition:",
                                "type": "string",
                                "format": "id"
                            }
                        },
                        "type": "object"
                    },
                    {
                        "$ref": "#/components/schemas/ProcessEditable"
                    },
                    {
                        "properties": {
                            "attributes": {
                                "description": "Process model\n\nSwagger definition:",
                                "properties": {
                                    "created_at": {
                                        "description": "Process model\n\nSwagger definition:",
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "updated_at": {
                                        "description": "Process model\n\nSwagger definition:",
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "id": {
                                        "description": "Process model\n\nSwagger definition:",
                                        "type": "string",
                                        "format": "id"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "type": "object"
                    }
                ]
            },
            "TestEditable": {
                "properties": {
                    "attributes": {
                        "description": "Test model\n\nSwagger definition:",
                        "properties": {
                            "data": {
                                "description": "Test model\n\nSwagger definition:",
                                "type": "object"
                            },
                            "status": {
                                "description": "Test model\n\nSwagger definition:",
                                "type": "string",
                                "enum": [
                                    "ACTIVE",
                                    "COMPLETED"
                                ]
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "Test": {
                "allOf": [
                    {
                        "properties": {
                            "id": {
                                "description": "Test model\n\nSwagger definition:",
                                "type": "string",
                                "format": "id"
                            }
                        },
                        "type": "object"
                    },
                    {
                        "$ref": "#/components/schemas/TestEditable"
                    },
                    {
                        "properties": {
                            "attributes": {
                                "description": "Test model\n\nSwagger definition:",
                                "properties": {
                                    "created_at": {
                                        "description": "Test model\n\nSwagger definition:",
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "updated_at": {
                                        "description": "Test model\n\nSwagger definition:",
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "id": {
                                        "description": "Test model\n\nSwagger definition:",
                                        "type": "string",
                                        "format": "id"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "type": "object"
                    }
                ]
            }
        }
    }
}