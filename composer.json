{
    "name": "coredump\/app",
    "type": "project",
    "description": "Coredump application.",
    "repositories": [
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/caleeli\/jdd-theme.git"
        },
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/caleeli\/jdd-flow.git"
        },
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/caleeli\/jdd-api.git"
        },
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/caleeli\/pacman.git"
        }
    ],
    "keywords": [
        "coredump",
        "application",
        "laravel"
    ],
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": "^7.1.3",
        "coredump/jdd-api": "dev-master",
        "coredump/jdd-flow": "dev-master",
        "coredump/jdd-pacman": "dev-master",
        "coredump/jdd-theme": "dev-master",
        "darkaonline/l5-swagger": "^5.8",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "beyondcode\/laravel-dump-server": "^1.0",
        "caleeli\/unit-test-doc": "dev-master",
        "filp\/whoops": "^2.0",
        "fzaninotto\/faker": "^1.4",
        "mockery\/mockery": "^1.0",
        "nunomaduro\/collision": "^3.0",
        "phpunit\/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app\/"
        },
        "classmap": [
            "database\/seeds",
            "database\/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "vendor\/bin\/phpunit -d memory_limit=-1 --coverage-html coverage",
            "vendor\/bin\/phpunit-doc > coverage\/doc.html"
        ],
        "post-install-cmd": [
            "@php -r \"!file_exists('database\/database.sqlite') && file_put_contents('database\/database.sqlite', '');\"",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi",
            "@php artisan storage:link"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan jdd-package:update"
        ],
        "post-root-package-install": [
            "@php -r \"!file_exists('database\/database.sqlite') && file_put_contents('database\/database.sqlite', '');\"",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link"
        ]
    }
}
