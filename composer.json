{
    "name": "coredump/manager",
    "type": "project",
    "description": "Coredump application manager.",
    "repositories": [
        {
            "type": "path",
            "url": "../vue-jdd-flow"
        },
        {
            "type": "path",
            "url": "../jdd-api"
        }
    ],
    "keywords": [
        "coredump",
        "manager",
        "laravel"
    ],
    "license": "GNU Affero General Public License version 3",
    "require": {
        "php": "^7.1.3",
        "coredump/jdd-api": "dev-master",
        "coredump/jdd-flow": "dev-master",
        "darkaonline/l5-swagger": "^5.8",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish"
        ],
        "post-root-package-install": [
            "@php -r \"!file_exists('database/database.sqlite') && file_put_contents('database/database.sqlite', '');\"",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
